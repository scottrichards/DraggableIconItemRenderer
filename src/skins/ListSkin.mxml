<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.List")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="disabled" />
		<s:State name="normal" />
	</s:states>
	
	<!-- SkinParts
	name=scroller, type=spark.components.Scroller, required=false
	name=dropIndicator, type=mx.core.IFactory, required=false
	name=dataGroup, type=spark.components.DataGroup, required=false
	-->
	
	
	<s:Group id="contentGroup" left="2" right="2" top="2" bottom="2">
		<s:layout>
			<s:VerticalLayout/>
		</s:layout>
	</s:Group>
	
	<s:Rect left="0" right="0" top="0" bottom="0" radiusX="10" radiusY="10">
		<s:stroke>
			<s:SolidColorStroke color="0x222222" weight="2"/>
		</s:stroke>
	</s:Rect>
</s:Skin>
